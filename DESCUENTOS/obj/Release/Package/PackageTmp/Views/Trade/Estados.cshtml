
@{
    ViewBag.Title = "Estado Nota";
    Layout = "~/Views/Shared/_LayoutPrincipal.cshtml";
}


<div class="row">
    <div class="col-md-12 ">
        <fieldset>
            <legend>
                <strong>Estado Nota Comercial</strong>
            </legend>
            <div class="row">

                <div class="col-md-12 mb-3">
                    <ul class="nav nav-tabs" id="myTab" role="tablist">
                        <li class="nav-item">
                            <a class="nav-link active" id="home-tab" data-toggle="tab" href="#home" role="tab" aria-controls="home" aria-selected="true"><strong>Diferencia de Precio</strong></a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" id="profile-tab" data-toggle="tab" href="#profile" role="tab" aria-controls="profile" aria-selected="false"><strong>Activación Comercia</strong>l</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" id="legal-tab" data-toggle="tab" href="#legal" role="tab" aria-controls="legal" aria-selected="false"><strong>Legalización Comercia</strong>l</a>
                        </li>
                    </ul>
                    <div class="tab-content" id="myTabContent">
                        <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">
                            <div id="div-grid-detalle"></div>
                        </div>
                        <div class="tab-pane fade" id="profile" role="tabpanel" aria-labelledby="profile-tab">
                            <div id="div-grid-activacion"></div>
                        </div>
                        <div class="tab-pane fade" id="legal" role="tabpanel" aria-labelledby="legal-tab">
                            <div id="div-grid-legal"></div>
                        </div>
                    </div>

                </div>


            </div>
        </fieldset>
    </div>
</div>
<script src="~/JS/Trade/Estados.js"></script>