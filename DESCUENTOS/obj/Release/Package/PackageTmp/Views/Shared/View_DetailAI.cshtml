
<div class="row m-3">
    <div class="col-md-12 ">
        <fieldset>
            <legend>
                <strong>Inventario Gloria</strong>
            </legend>
            <div class="row">
                <div class="col-sm-5">
                    <div class="input-group mb-3">
                        <div class="input-group-prepend">
                            <span class="input-group-text">
                                <strong>Supervisor Logística</strong>
                            </span>
                        </div>
                        <input id="txtusuario" type="text" class="form-control" required disabled>
                    </div>
                </div>

                <div class="col-sm-3">
                    <div class="input-group mb-3">
                        <div class="input-group-prepend">
                            <span class="input-group-text">
                                <strong>Documento SAP</strong>
                            </span>
                        </div>
                        <input id="txtdocsap" type="text" class="form-control" required disabled>
                    </div>
                </div>


                <div class="col-sm-4">
                    <div class="input-group mb-3">
                        <div class="input-group-prepend">
                            <span class="input-group-text">
                                <strong>Fecha Inventario</strong>
                            </span>
                        </div>
                        <input id="txtfeinv" type="date" class="form-control" required disabled>
                    </div>
                </div>

                <div class="col-sm-5">
                    <div class="input-group mb-3">
                        <div class="input-group-prepend">
                            <span class="input-group-text">
                                <strong>Centro de Distribución</strong>
                            </span>
                        </div>
                        <input id="txtcentro" type="text" class="form-control" required disabled>
                    </div>
                </div>

                <div class="col-sm-3">
                    <div class="input-group mb-3">
                        <div class="input-group-prepend">
                            <span class="input-group-text">
                                <strong>Cantidad SKU</strong>
                            </span>
                        </div>
                        <input id="txtcantsku" type="text" class="form-control" required disabled>
                    </div>
                </div>


                <div class="col-sm-4">
                    <div class="input-group mb-3">
                        <div class="input-group-prepend">
                            <span class="input-group-text">
                                <strong>Valor Total Inventario</strong>
                            </span>
                        </div>
                        <input id="txttotalinv" type="text" class="form-control" required disabled>
                    </div>
                </div>

                <div class="col-md-12">
                    <div class="mb-3" id="div-grid-product">
                    </div>
                </div>
                <div class="col-md-12">
                    <div class="mb-3">
                        <div class="card-footer text-center">
                            <strong>Justificación </strong>
                        </div>
                        <textarea id="txtmot" class="form-control" required disabled></textarea>
                    </div>
                </div>
            </div>
        </fieldset>
    </div>
    <div class="col-md-12">
        <fieldset>
            <legend>
                <strong>Indicadores</strong>
            </legend>

            <div class="row">
                <div class="col-sm-4">
                    <div class="input-group mb-3">
                        <div class="input-group-prepend">
                            <span class="input-group-text">
                                <strong>Total Diferencia Inventario</strong>
                            </span>
                        </div>
                        <input id="txtdifinv" type="text" class="form-control" required disabled>
                    </div>
                </div>
                <div class="col-sm-4">
                    <div class="input-group mb-3">
                        <div class="input-group-prepend">
                            <span class="input-group-text">
                                <strong>% de Diferencia</strong>
                            </span>
                        </div>
                        <input id="txtpdiferencia" type="text" class="form-control" required disabled>
                    </div>
                </div>
                <div class="col-sm-4">
                    <div class="input-group mb-3">
                        <div class="input-group-prepend">
                            <span class="input-group-text">
                                <strong>% Exactitud de Inventario</strong>
                            </span>
                        </div>
                        <input id="txtpexactitud" type="text" class="form-control" required disabled>
                    </div>
                </div>
            </div>
        </fieldset>
    </div>
    <div class="col-md-12">
        <fieldset>
            <legend>
                <strong>Soportes Adjuntos</strong>
            </legend>
            <div class="row">
                <div class="col-sm-4">
                    <div class="mb-3" id="div-grid-files">
                    </div>
                </div>
            </div>
            
        </fieldset>
    </div>
</div>