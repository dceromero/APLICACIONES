
@{
    ViewBag.Title = "Informe Solicitudes Notas";
    Layout = "~/Views/Shared/_LayoutPrincipal.cshtml";
}

<div class="row">
    <div class="col-md-12 ">
        <fieldset>
            <legend>
                <strong>Informe de Solicitudes Notas de Activación Comercial</strong>
            </legend>
            <div class="row">
                <div class="col-md-5">
                    <div class="input-group mb-3">
                        <div class="input-group-prepend">
                            <span class="input-group-text">
                                <strong>Fecha Inicial</strong>
                            </span>
                        </div>
                        <input id="txtfecini" type="date" class="form-control" required>
                    </div>
                </div>
                <div class="col-md-5">
                    <div class="input-group mb-3">
                        <div class="input-group-prepend">
                            <span class="input-group-text">
                                <strong>Fecha Final</strong>
                            </span>
                        </div>
                        <input id="txtfecfin" type="date" class="form-control" required>
                    </div>
                </div>
                <div class="col-md-2">
                    <button id="btn-reptsolicitud" class="btn  btn-block btn-outline-info">Buscar</button>
                </div>
                <div class="col-md-12">
                    <div id="div-grid" style="width:200%"></div>
                </div>
            </div>
        </fieldset>
    </div>
</div>
@Html.Partial("~/Views/Shared/ViewMessages.cshtml")

<script src="~/JS/Trade/InformeSolicitudes.js"></script>


