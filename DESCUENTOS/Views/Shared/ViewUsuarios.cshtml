            <span class="input-group-text" id="basic-addon1">
                <strong>Empresa</strong>
            </span>
        </div>
        <select id="cmbemp" class="form-control" required></select>
    </div>
</div>
<div class="col-md-4">
    <div class="input-group mb-3">
        <div class="input-group-prepend">
            <span class="input-group-text" id="basic-addon1">
                <strong>Cedula</strong>
            </span>
        </div>
        <input id="txtcedula" type="number" class="form-control" placeholder="Solo Números" required>
    </div>
</div>
<div class="col-md-4">
    <div class="input-group mb-3">
        <div class="input-group-prepend">
            <span class="input-group-text" id="basic-addon1">
                <strong>Codigo</strong>
            </span>
        </div>
        <input id="txtcod" type="number" class="form-control" placeholder="Cod Emp o C.C." required>
    </div>
</div>

<div class="col-md-6">
    <div class="input-group mb-3">
        <div class="input-group-prepend">
            <span class="input-group-text" id="basic-addon1">
                <strong>Nombres</strong>
            </span>
        </div>
        <input id="txtnombres" type="text" class="form-control" required>
    </div>

    <div class="input-group mb-3">
        <div class="input-group-prepend">
            <span class="input-group-text" id="basic-addon1">
                <strong>Mail</strong>
            </span>
        </div>
        <input id="txtmail" type="email" class="form-control" required>
    </div>

    <div class="input-group mb-3">
        <div class="input-group-prepend">
            <span class="input-group-text" id="basic-addon1">
                <strong>Centro Costo</strong>
            </span>
        </div>
        <input id="txtcencos" type="text" class="form-control" required>
        <input id="lblcencos" type="hidden" />
    </div>

    <div class="input-group mb-3">
        <div class="input-group-prepend">
            <span class="input-group-text" id="basic-addon1">
                <strong>Movil</strong>
            </span>
        </div>
        <input id="txtmovil" type="number" class="form-control" required>
    </div>
</div>

<div class="col-md-6">

    <div class="input-group mb-3">
        <div class="input-group-prepend">
            <span class="input-group-text" id="basic-addon1">
                <strong>Apellidos</strong>
            </span>
        </div>
        <input id="txtapel" type="text" class="form-control" required>
    </div>

    <div class="input-group mb-3">
        <div class="input-group-prepend">
            <span class="input-group-text" id="basic-addon1">
